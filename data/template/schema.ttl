bas:BioassayType  a         bat:Assignment ;
        rdfs:label          "bioassay type" ;
        bat:hasDescription  "Categorization of bioassays based on the property or process that the assay is interrogating, e.g. ADMET, functional, binding, etc.; as well as the general class of bioassay (enyzme activity, gene expression...)" ;
        bat:hasProperty     bao:BAX_0000002 ;
        bat:hasValue        [ rdfs:label          "bioassay" ;
                              bat:hasDescription  "A set of instructions, methodology, operations, required reagents, instruments to carrie out experiments for the purpose of testing the effect of a perturbing agent in a biological model system, measuring one or multiple effect(s) of the agent facilitated by an assay design method translate the perturbation into a detectable signal to arrive at one or multiple endpoint(s) that quantify or qualify the extent of the perturbation.  Bioassay is described by multiple bioassay components: assay format, biology (biological participants in various role and processes), design method, physical detection method / technology, screened entity, and endpoint. Bioassay includes one or multiple measure groups to describe panel, profiling, multiparametric (or multiplexed) assays (assays that measure more than one effect of the perturbagen on the system that is screened)." ;
                              bat:inOrder         "1"^^xsd:int ;
                              bat:isWholeBranch   true ;
                              bat:mapsTo          bao:BAO_0000015
                            ] ;
        bat:hasValue        [ rdfs:label          "assay kit" ;
                              bat:hasDescription  "The title of the kit/biologicals used in the assay." ;
                              bat:inOrder         "6"^^xsd:int ;
                              bat:isExclude       true ;
                              bat:mapsTo          bao:BAO_0000248
                            ] ;
        bat:hasValue        [ rdfs:label          "measure group specification" ;
                              bat:hasDescription  "An abstract concept to group one or more assay readout and allow description of an assay that measures one or more effect of a perturbagen on the biological entity.  This does not define the physical readout method itself, which is captured in assay design, but rather classifies the data being generated by those readouts." ;
                              bat:inOrder         "10"^^xsd:int ;
                              bat:isExclude       true ;
                              bat:mapsTo          bao:BAO_0002185
                            ] ;
        bat:hasValue        [ rdfs:label     "experimental specification" ;
                              bat:inOrder    "7"^^xsd:int ;
                              bat:isExclude  true ;
                              bat:mapsTo     bao:BAO_0002933
                            ] ;
        bat:hasValue        [ rdfs:label          "measure group" ;
                              bat:hasDescription  "Measure group is used to group and link multiple (different) sets of experimental results to one bioassay. Measure groups thus facilitate the description of multiparametric (or multiplexed) assays, that measure more than one effect of the perturbagen on the system that is screened. It also facilitate the definition of panel and profiling assays, e.g. assays that test compounds against multiple (protein) targets.\nCompare definition of bioassay.\nMeasure group links to those classes that can vary among different measurements of a panel assay, profiling assay, multiparametric assay, e.g. target, assay design method, physical detection method, endpoint. Measure group is also used to define derived endpoints, for example concentration-response endpoints obtained by curve fitting a set of results from the same compound tested in the same assay at different concentrations." ;
                              bat:inOrder         "9"^^xsd:int ;
                              bat:isExclude       true ;
                              bat:mapsTo          bao:BAO_0000040
                            ] ;
        bat:hasValue        [ rdfs:label          "bioassay type" ;
                              bat:hasDescription  "Categorization of bioassays based on the property or process that the assay is interrogating, e.g. ADMET, functional, etc." ;
                              bat:inOrder         "2"^^xsd:int ;
                              bat:isWholeBranch   true ;
                              bat:mapsTo          bao:BAO_0000008
                            ] ;
        bat:hasValue        [ rdfs:label          "not determined" ;
                              bat:hasDescription  "The measurement was not made." ;
                              bat:inOrder         "4"^^xsd:int ;
                              bat:isExclude       true ;
                              bat:mapsTo          bat:NotDetermined
                            ] ;
        bat:hasValue        [ rdfs:label          "bioassay specification" ;
                              bat:hasDescription  "Description of an experiment carried out for the purpose of screening a perturbing agent in a biological system, measuring the effect of the agents using specified technology, and arriving at endpoints that satisfy particular criteria." ;
                              bat:inOrder         "8"^^xsd:int ;
                              bat:isExclude       true ;
                              bat:mapsTo          bao:BAO_0000026
                            ] ;
        bat:hasValue        [ rdfs:label          "absence" ;
                              bat:hasDescription  "A group of annotations that explain why an annotation is missing, when it should not be." ;
                              bat:inOrder         "3"^^xsd:int ;
                              bat:isWholeBranch   true ;
                              bat:mapsTo          bat:Absence
                            ] ;
        bat:hasValue        [ rdfs:label          "see Gene ID" ;
                              bat:hasDescription  "The target is present in the Gene ID section, but needs to be mapped to a formal ontology." ;
                              bat:inOrder         "5"^^xsd:int ;
                              bat:isExclude       true ;
                              bat:mapsTo          bat:SeeGeneID
                            ] ;
        bat:inOrder         "1"^^xsd:int .

