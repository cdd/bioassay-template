                            ] ;
        bat:hasValue        [ rdfs:label          "raw activity" ;
                              bat:hasDescription  "Raw or qualitative data are reported as endpoints in an assay, e.g., luminescence counts, fluorescence intensity, etc." ;
                              bat:inOrder         "79"^^xsd:int ;
